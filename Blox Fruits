local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "Blox-Fruit | Gamehub", HidePremium = false, SaveConfig = true, ConfigFolder = "game-BF"})

-- UI

local Tab = Window:MakeTab({
	Name = "Scripts",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Tab2 = Window:MakeTab({
	Name = "Fruit Finder",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

Tab:AddButton({
	Name = "Fruit Finder",
	Callback = function()
        loadstring(game:HttpGet("https://pastebin.com/raw/7msAzwYb"))()
  	end    
})

Tab2:AddButton({
	Name = "Fruit Finder",
	Callback = function()
        repeat wait(5) until game:IsLoaded()
